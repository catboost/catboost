

PREBUILT_PROGRAM(protoc)

IF (HOST_OS_DARWIN AND HOST_ARCH_X86_64 OR
    HOST_OS_LINUX AND HOST_ARCH_X86_64 OR
    HOST_OS_WINDOWS AND HOST_ARCH_X86_64)
ELSE()
    MESSAGE(FATAL_ERROR Unsupported host platform for prebuilt arcadia protoc_std)
ENDIF()

DECLARE_EXTERNAL_HOST_RESOURCES_BUNDLE(
    ARCADIA_PROTOC_STD
    sbr:1601304308 FOR DARWIN
    sbr:1601304567 FOR LINUX
    sbr:1601304404 FOR WIN32
)

PRIMARY_OUTPUT(${ARCADIA_PROTOC_STD_RESOURCE_GLOBAL}/protoc${MODULE_SUFFIX})

END()
